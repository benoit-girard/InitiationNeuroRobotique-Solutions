<!--node thymio-II-->
<node nodeId="51938" name="thymio-II">

var Neurones[2]


call leds.top(32,26,6)


onevent prox
	Neurones[0] = ((prox.horizontal[5] / 6 + prox.horizontal[6] / 6) + Neurones[1]) - prox.horizontal[2] / 6
	if Neurones[0] &lt; 0 then
		Neurones[0] = 0
	elseif Neurones[0] &gt; 500 then
		Neurones[0] = 500
	end
	Neurones[1] = Neurones[0] - prox.horizontal[2] / 6
	if Neurones[1] &lt; 0 then
		Neurones[1] = 0
	elseif Neurones[1] &gt; 500 then
		Neurones[1] = 500
	end
	call leds.circle(0,0,Neurones[1] / 16,0,0,0,Neurones[0] / 16,0)
	motor.left.target = Neurones[0]
	motor.right.target = Neurones[1]


